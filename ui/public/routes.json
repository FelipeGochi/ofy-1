        },
        {
            "route": "/objective/*",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/signup/*",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/password-recovery/*",
            "serve": "/index.html",
